{
  "$schema": "../app-info-schema.json",
  "name": "Nextcloud",
  "available": true,
  "exposable": true,
  "port": 8083,
  "id": "nextcloud",
  "tipi_version": 24,
  "version": "26.0.13-apache",
  "categories": ["data"],
  "description": "Nextcloud is a self-hosted, open source, and fully-featured cloud storage solution for your personal files, office documents, and photos.",
  "short_desc": "Productivity platform that keeps you in control",
  "author": "Nextcloud GmbH",
  "source": "https://github.com/nextcloud/server",
  "form_fields": [
    {
      "type": "text",
      "label": "Username",
      "max": 50,
      "min": 3,
      "required": true,
      "env_variable": "NEXTCLOUD_ADMIN_USER"
    },
    {
      "type": "password",
      "label": "Password",
      "max": 50,
      "min": 10,
      "required": true,
      "env_variable": "NEXTCLOUD_ADMIN_PASSWORD"
    },
    {
      "type": "text",
      "label": "Database Host",
      "max": 50,
      "min": 3,
      "required": true,
      "env_variable": "NEXTCLOUD_DB_HOST"
    },
    {
      "type": "text",
      "label": "Database User",
      "max": 50,
      "min": 3,
      "required": true,
      "env_variable": "NEXTCLOUD_DB_USER"
    },
    {
      "type": "password",
      "label": "Database Password",
      "max": 50,
      "min": 10,
      "required": true,
      "env_variable": "NEXTCLOUD_DB_PASSWORD"
    },
    {
      "type": "email",
      "label": "Admin Email",
      "max": 100,
      "min": 5,
      "required": true,
      "env_variable": "NEXTCLOUD_ADMIN_EMAIL"
    },
    {
      "type": "boolean",
      "label": "Enable HTTPS",
      "required": false,
      "env_variable": "NEXTCLOUD_ENABLE_HTTPS"
    },
    {
      "type": "boolean",
      "label": "Enable NVIDIA GPU",
      "required": false,
      "env_variable": "NEXTCLOUD_ENABLE_NVIDIA_GPU"
    },
    {
      "type": "text",
      "label": "Additional PHP Extensions",
      "required": false,
      "env_variable": "NEXTCLOUD_ADDITIONAL_PHP_EXTENSIONS"
    },
    {
      "type": "text",
      "label": "Additional APKs",
      "required": false,
      "env_variable": "NEXTCLOUD_ADDITIONAL_APKS"
    },
    {
      "type": "text",
      "label": "Additional Community Containers",
      "required": false,
      "env_variable": "AIO_COMMUNITY_CONTAINERS",
      "placeholder": "e.g., fail2ban pi-hole plex",
      "hint": "Specify space-separated community container names to include. Refer to the community-containers directory for available options."
    }
  ],
  "supported_architectures": ["arm64", "amd64"],
  "created_at": 1691943801422,
  "updated_at": 1739719088000
}
